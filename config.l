;; My tmacs config file.

;; Set some variables.
(set-int tab-width 4)
(set-int vsync 1)
(set-int smooth-scroll 1)
(set-int font-size 17)
(set-int draw-text-blended 1)

(set-float scroll-speed 0.035)

;; Set font
(set-string font "fonts/lucon.ttf")

(set-string str font)
(print str)

(set-float test 15.0)
(set-float a 0.0)

(set-float a test)
(+ a test)
(print a)
